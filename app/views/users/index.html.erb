<div class="row">
  <div class="col-sm-10 col-sm-offset-1">
    <h3>PEOPLE</h3>
    <% if @users.present? %>
      <div id="users">
        <% @users.each do |user| %>
          <p class="search-result">
            <%= link_to image_tag(user.image, height: '30'), user_path(user) %>
            <%= link_to user.name, user_path(user) %>
          </p>
          <br />
        <% end %>
      </div>
    <% end %>
  </div>
</div>  
<div class="row">
  <div class="col-sm-11">
  <%= paginate @users %>
  </div>
</div>

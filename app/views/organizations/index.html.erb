<div class="row">
  <div class="col-sm-10 col-sm-offset-1">
    <h3>ORGANIZATIONS</h3>
    <% if @organizations.present? %>
      <div id="organizations">
        <% @organizations.each do |org| %>
          <p class="search-result">
            <%= link_to org.name, organization_path(org) %>
          </p>
          <br />
        <% end %>
      </div>
    <% end %>
  </div>
</div>  
<div class="row">
  <div class="col-sm-11">
  <%= paginate @organizations %>
  </div>
</div>

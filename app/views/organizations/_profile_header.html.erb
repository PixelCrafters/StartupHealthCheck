<div class="row">
  <div class="col-sm-12">
    <div id="profile-header">
      <h3><%= @organization.name %></h3>
      <% if @organization.main_address %>
        <span class="right"><%= @organization.main_address.address1 %>, Vancouver, <%= @organization.main_address.state %>, <%= @organization.main_address.zip %></span>
      <% end %>
      <br />
      <h5><%= @organization.headline %></h5>
      <span>status: <%= status %></span>
      <span class="right"><%= link_to @organization.url, @organization.url %></span>
    </div>
    This profile was claimed by: <%= link_to @organization.users.first.name, user_path(@organization.users.first.name) %>
  </div>
</div>
